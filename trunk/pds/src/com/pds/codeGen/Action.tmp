import javax.annotation.Resource;

import org.springframework.stereotype.Controller;

import com.opensymphony.xwork2.Action;
import com.pds.core.BaseAction;
import com.pds.service.{Entity}Service;

/**
 * @author qiaoxueshi
 *
 */
@Controller
public class {Entity}Action extends BaseAction {
	private static final long serialVersionUID = 5495728211877266147L;

	@Resource
	private {Entity}Service service ;
	
	public {Entity}Service getService() {
		return service;
	}
	public void setService({Entity}Service service) {
		this.service = service;
	}
	
}
